
.rigthInfo{
	float: right;
	font-size: 25rpx;
	text-align: right;
}
.payCount{
	margin-top: 15rpx;
}
.midInfo{
	float: left;
	margin-left: 20rpx;
}
.phone{
	font-size: 25rpx;
	margin-top: 15rpx;
}
.vipLevel{
	font-size: 25rpx;
	margin-top: 15rpx;
}
.title{
	font-size: 35rpx;
	font-weight: 700;
}
.eachVip{
	width: 100%;
	height: 150rpx;
	overflow: hidden;
	margin-top: 30rpx;
	position: relative;
}
.leftImg{
	width: 150rpx;
	height: 150rpx;
	float: left;
	/* top: 0;
	left: 0;
	position: absolute; */
	border-radius: 15rpx;
	overflow: hidden;
}
.eachVip image{
	width: 100%;
	height: 100%;
	display: block;
}
.topTitle{
	font-size: 35rpx;
	font-weight: 700;
}
.vipList{
	width: 92%;
	margin: 0 auto;
	margin-top: 40rpx;
}
.holePage{
	width: 100%;
}
.holePage image{
	width: 100%;
	display: block;
}
.topBg{
	width: 100%;
	height: 150rpx;
}
.goBack{
	width: 40rpx;
	height: 40rpx;
	position: fixed;
	top: 80rpx;
	left: 20rpx;
}
.goBack image{
	width: 100%;
	height: 100%;
	display: block;
}
.moreIcon{
	width: 40rpx;
	/* height: 40upx; */
	position: fixed;
	top: 85rpx;
	right: 30rpx;
}
.moreIcon image{
	width: 100%;
	height: 100%;
	display: block;
}
.topHeadImg{
	width: 150rpx;
	height: 150rpx;
	position: absolute;
	left: 300rpx;
	top: 130rpx;
	border-radius: 15rpx;
}
.topHeadImg image{
	width: 100%;
	height: 100%;
	display: block;
}
.shopInfo{
	width: 100%;
	padding-top: 80rpx;
}
.shopName{
	width: 100%;
	text-align: center;
	font-size: 40rpx;
	font-weight: 700;
}
.shopDetail{
	width: 100%;
	text-align: center;
	font-size: 20rpx;
	color: #717171;
	margin-top: 10rpx;
}
.couponView{
	width: 100%;
	overflow: hidden;
	margin-top: 20rpx;
}
.leftCou{
	width: 300rpx;
	height: 60rpx;
	margin-left: 70rpx;
	float: left;
	position: relative;
}
.leftCou image{
	width: 100%;
	height: 100%;
	display: block;
}
.leftCou .left{
	width: 220rpx;
	line-height: 60rpx;
	color: #FFFFFF;
	font-size: 25rpx;
	left: 0rpx;
	top: 0rpx;
	position: absolute;
	/* background-color: #000000; */
	text-align: center;
}
.leftCou .right{
	width: 80rpx;
	line-height: 60rpx;
	color: #FFFFFF;
	font-size: 25rpx;
	right: 0rpx;
	top: 0rpx;
	position: absolute;
	/* background-color: #000000; */
	text-align: center;
}
.addView{
	width: 85%;
	overflow: hidden;
	margin: 0 auto;
	height: 40rpx;
	margin-top: 15rpx;
	color: #8A8A8A;
	font-size: 22rpx;
}
.addView .left{
	float: left;
	position: relative;
	width: 80%;
	height: 100%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.addView .left image{
	width: 20rpx;
	height: 20rpx;
	display: block;
	position: absolute;
	left: 0;
	top: 10rpx;
}
.addView .left text{
	/* font-size: 20upx; */
	line-height: 40rpx;
	position: absolute;
	left: 30rpx;
}
.addView .right{
	width: 20%;
	float: left;
	height: 100%;
	position: relative;
}
.addView .right image{
	width: 20rpx;
	height: 20rpx;
	display: block;
	position: absolute;
	right: 0rpx;
	top: 10rpx;
}
.addView .right text{
	position: absolute;
	right: 30rpx;
	/* font-size: 20upx; */
	line-height: 40rpx;
}


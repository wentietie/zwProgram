
.subBtn {
	width: 90%;
	margin: 40rpx auto;
	line-height: 100rpx;
	/* margin-top: 40upx; */
	border-radius: 20rpx;
	text-align: center;
	color: #FFFFFF;
	/* border-color: #edca8d; */
	background-color: #EDCA8D;
	font-size: 35rpx;
}
.imageBottom {
	width: 90%;
	margin: 0 auto;
	margin-top: 30rpx;
	padding-bottom: 40rpx;
}
.imageList {
	width: 100%;
	overflow: hidden;
}
.eachImg {
	width: 30%;
	height: 200rpx;
	float: left;
	margin-left: 5%;
	margin-top: 20rpx;
}
.eachImg image {
	width: 100%;
	height: 100%;
	display: block;
}
.eachInput image {
	width: 30rpx;
	height: 30rpx;
	position: absolute;
	top: 50rpx;
	right: 0;
}
.bottomLabel {
	width: 100%;
	line-height: 50rpx;
	font-size: 35rpx;
	font-weight: 600;
	margin-top: 5rpx;
}
.topLabel {
	width: 100%;
	font-size: 25rpx;
	color: #8c8c8c;
}
.eachInput {
	width: 90%;
	margin: 0 auto;
	margin-top: 30rpx;
	border-bottom: 1rpx solid #e6e6e6;
	padding-bottom: 15rpx;
	position: relative;
}
.holePage {
	width: 100%;
}

